    "freq_2015_displayed = before_selected['Fandango_Stars'].value_counts(normalize=True).sort_index() * 100\n",
    "freq_2016_displayed = after_selected['fandango'].value_counts(normalize=True).sort_index() * 100\n",
    "\n",
    "# Plot\n",
    "plt.figure(figsize=(10,6))\n",
    "plt.plot(freq_2015_displayed.index, freq_2015_displayed.values, marker='o', color='orange', label='2015 Displayed Ratings')\n",
    "plt.plot(freq_2016_displayed.index, freq_2016_displayed.values, marker='o', color='green', label='2016-17 Displayed Ratings')\n",
    "\n",
    "plt.title('Comparison of Fandango Displayed Ratings (2015 vs 2016-17)')\n",
    "plt.xlabel('Stars')\n",
    "plt.ylabel('Percentage of Movies')\n",
    "plt.xticks([0,0.5,1,1.5,2,2.5,3,3.5,4,4.5,5])\n",
    "plt.legend()\n",
    "plt.grid(True)\n",
    "plt.show()\n"
